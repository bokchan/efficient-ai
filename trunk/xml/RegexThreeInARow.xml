<?xml version="1.0" encoding="UTF-8"?>
<expressions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="RegexExpression.xsd">
  <expression enabled="true">
  	<description>Vertical</description>
    <pattern>(VAR[0-2]{6}){3}</pattern>
    <offsetY>3</offsetY>
    <offsetX>0</offsetX>
    <modMin>0</modMin>
    <modMax>6</modMax>    
  </expression>  
  <expression enabled="true">
  	<description>Horisontal 1110</description>
    <pattern>VAR{3}0{1,}</pattern>
    <offsetY>0</offsetY>
    <offsetX>3</offsetX>
    <modMin>0</modMin>
    <modMax>3</modMax>
  </expression>
  <expression enabled="true">
  	<description>Horisontal 0111</description>
    <pattern>0VAR{3}</pattern>
    <offsetY>0</offsetY>
    <offsetX>0</offsetX>
    <modMin>0</modMin>
    <modMax>3</modMax>
  </expression>
  <expression enabled="true">
  	<description>Horisontal 1101</description>
    <pattern>VARVAR0VAR</pattern>
    <offsetY>0</offsetY>
    <offsetX>2</offsetX>
    <modMin>0</modMin>
    <modMax>3</modMax>
  </expression>
  <expression enabled="true">
  	<description>Horisontal 1011</description>
    <pattern>VAR0VARVAR</pattern>
    <offsetY>0</offsetY>
    <offsetX>1</offsetX>
    <modMin>0</modMin>
    <modMax>3</modMax>
  </expression>
  <expression enabled="true">
  	<description>Diagonal increasing 1110</description>
    <pattern>(VAR[0-2]{7}){3}0</pattern>
    <offsetY>3</offsetY>
    <offsetX>3</offsetX>
    <modMin>0</modMin>
    <modMax>3</modMax>
  </expression>
  
  <expression enabled="true">
  	<description>Diagonal increasing 0111</description>
    <pattern>0[0-2]{7}(VAR[0-2]{7}){3}</pattern>
    <offsetY>0</offsetY>
    <offsetX>0</offsetX>
    <modMin>0</modMin>
    <modMax>3</modMax>
    
  </expression>
  
  <expression enabled="true">
  	<description>Diagonal increasing 1101</description>
    <pattern>(VAR[0-2]{7}){2}0[0-2]{7}VAR</pattern>
    <offsetY>2</offsetY>
    <offsetX>2</offsetX>
    <modMin>0</modMin>
    <modMax>3</modMax>
  </expression>
  
  <expression enabled="true">
  	<description>Diagonal increasing 1011</description>
    <pattern>(VAR[0-2]{7})0[0-2]{7}VAR[0-2]{7}VAR</pattern>
    <offsetY>1</offsetY>
    <offsetX>1</offsetX>
    <modMin>0</modMin>
    <modMax>3</modMax>
  </expression>
  
<expression enabled="true">
  	<description>Diagonal descreasing 1110</description>
    <pattern>(VAR[0-2]{5}){3}0</pattern>
    <offsetY>3</offsetY>
    <offsetX>-3</offsetX>
    <modMin>3</modMin>
    <modMax>6</modMax>
  </expression>
  
  <expression enabled="true">
  	<description>Diagonal descreasing 0111</description>
    <pattern>0[0-2]{5}(VAR[0-2]{5}){3}</pattern>
    <offsetY>0</offsetY>
    <offsetX>0</offsetX>
    <modMin>3</modMin>
    <modMax>6</modMax>
  </expression>
<expression enabled="true">
  	<description>Diagonal descreasing 1011</description>
    <pattern>VAR[0-2]{5}0[0-2]{5}VAR[0-2]{5}1</pattern>
    <offsetY>1</offsetY>
    <offsetX>-1</offsetX>
    <modMin>3</modMin>
    <modMax>6</modMax>
  </expression>
  <expression enabled="true">
  	<description>Diagonal descreasing 1101</description>
    <pattern>(VAR[0-2]{5}){2}0[0-2]{5}VAR</pattern>
    <offsetY>2</offsetY>
    <offsetX>-2</offsetX>
    <modMin>3</modMin>
    <modMax>6</modMax>
  </expression>    
</expressions>